<?xml version= "1.0" encoding ="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >


<mapper namespace="com.springmvc.api.persistence.cardDao">
	<select id="getCardInfo" resultType="cardEntity">
		SELECT * FROM card WHERE card_number=#{mycardnumber}
	</select>

	<select id="setCardInfo" resultType="cardEntity">
		INSERT
		card
		SET
		card_number = #{card_number},
		pass_word = #{pass_word},
		card_token = #{card_token}
	</select>
</mapper>       